{
	"name": "hackathon",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"generate:css": "unocss 'src/**/*.tsx' --out-file ./styles/unocss.css --config ./unocss.config.ts",
		"dev": "concurrently 'yarn generate:css --watch' 'next dev'",
		"build": "yarn generate:css && next build",
		"start": "next start",
		"lint": "TIMING=1 next lint --fix",
		"format": "prettier --write --cache ."
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@emotion/server": "^11.10.0",
		"@mantine/core": "^5.9.0",
		"@mantine/hooks": "^5.9.0",
		"@mantine/next": "^5.9.0",
		"@types/node": "18.11.10",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.9",
		"@unocss/reset": "^0.47.5",
		"eslint": "8.29.0",
		"eslint-config-next": "13.0.6",
		"framer-motion": "^7.6.18",
		"next": "13.0.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "4.9.3"
	},
	"devDependencies": {
		"@types/prettier": "^2",
		"@unocss/cli": "^0.47.5",
		"concurrently": "^7.6.0",
		"eslint-config-mahir": "^0.0.18",
		"prettier": "^2.8.0",
		"unocss": "^0.47.5"
	},
	"packageManager": "yarn@3.3.0"
}
